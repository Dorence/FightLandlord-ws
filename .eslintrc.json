{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "indent": [
            "warn",
            4,
            { "SwitchCase": 1, "ObjectExpression": "off" }
        ],
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-mixed-spaces-and-tabs": "error",
        "eqeqeq": "error",
        "strict": "error",
        "no-console": "off",
        "no-extra-boolean-cast": "off"
    },
    "globals": { "$": true, "define": true }
}